{
  "publishConfig": {
    "access": "public"
  },
  "version": "0.9.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@iii8iii/dfcfbot",
  "author": "iii8iii",
  "module": "dist/dfcfbot.esm.js",
  "size-limit": [
    {
      "path": "dist/dfcfbot.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dfcfbot.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "delay": "^5.0.0",
    "got": "^11.8.2"
  }
}
